			};
		};

		pmx_sdc3_clk {
			qcom,pins = <&sdc 7>;
			qcom,num-grp-pins = <1>;
			label = "sdc3-clk";
			sdc3_clk_on: clk_on {
				bias-disable; /* NO pull */
				drive-strength = <6>; /* 6 MA */
			};
			sdc3_clk_off: clk_off {
				bias-disable; /* NO pull */
				drive-strength = <2>; /* 2 MA */
			};
		};

		pmx_sdc3_cmd {
			qcom,pins = <&sdc 8>;
			qcom,num-grp-pins = <1>;
			label = "sdc3-cmd";
			sdc3_cmd_on: cmd_on {
				bias-pull-up; /* pull up */
				drive-strength = <6>; /* 6 MA */
			};
			sdc3_cmd_off: cmd_off {
				bias-pull-up; /* pull up */
