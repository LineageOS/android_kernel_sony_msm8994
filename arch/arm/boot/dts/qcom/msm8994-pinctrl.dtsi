				bias-disable;
			};

			i2c_5_sleep: i2c_5_sleep {
				drive-strength = <2>;
				bias-disable;
			};
		};

		pmx_fm_int_active {
			qcom,pins = <&gp 9>;
			qcom,pin-func = <0>;
			qcom,num-grp-pins = <1>;
			label = "pmx_fm_int_active";

			fm_int_active: fm_int_active {
				drive-strength = <16>;
				bias-pull-up;
			};
		};

		pmx_fm_int_suspend {
			qcom,pins = <&gp 9>;
			qcom,pin-func = <0>;
			qcom,num-grp-pins = <1>;
			label = "pmx_fm_int_suspend";

			fm_int_suspend: fm_int_suspend {
				drive-strength = <16>;
				bias-pull-up;
			};
		};

		pmx_fm_status_int_active {
			qcom,pins = <&gp 11>;
			qcom,pin-func = <0>;
			qcom,num-grp-pins = <1>;
			label = "pmx_fm_status_int_active";

			fm_status_int_active: fm_status_int_active {
